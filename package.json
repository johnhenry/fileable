{
  "name": "fileable",
  "version": "0.0.0",
  "description": "",
  "main": "./dist/lib/index.js",
  "bin": {
    "fileable": "./dist/cli/fileable.js"
  },
  "directories": {
    "example": "example"
  },
  "scripts": {
    "fileable": "./dist/cli/fileable.js",
    "jsdoc-md": "jsdoc-md",
    "lint-no-fix": "tslint --project src",
    "lint": "npm run lint-no-fix -- --fix",
    "build-lib-cli": "rollup --silent --config config/rollup.config.js",
    "build-js-docs": "sh ./scripts/build-js-docs.sh",
    "build-docs": "npm run build-js-docs && cat ./static/docs/readme.md ./dist/temp/api.md ./static/docs/todo.md > ./readme.md",
    "build": "npm run lint && npm run build-lib-cli && npm run build-docs",
    "test-no-build": "node test/index.js",
    "test": "npm run build-lib-cli && npm run test-no-build",
    "demo-no-deps": "node test/demo-human-input.js",
    "demo": "npm run build-lib-cli && npm run demo-no-deps",
    "add-built": "git add dist/ && git add src/ && git add readme.md",
    "start": "npm run demo && live-server dist/temp/"
  },
  "pre-commit": [
    "test",
    "add-built"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "7.2.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "babel-plugin-dynamic-import-node-babel-7": "2.0.7",
    "find-up": "3.0.0",
    "jszip": "3.2.0",
    "node-fetch": "2.3.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "rimraf": "2.6.3",
    "rollup": "1.6.0",
    "rollup-plugin-hashbang": "2.2.2",
    "yargs": "13.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "jsdoc-md": "2.1.0",
    "live-server": "1.2.1",
    "node-popup": "0.1.14",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.3",
    "rollup-plugin-babel": "4.3.2",
    "tape": "4.10.1",
    "tslint": "5.14.0",
    "typescript": "3.3.3333"
  }
}
